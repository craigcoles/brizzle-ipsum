{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "dev": {
      "dependsOn": [
        "^build"
      ],
      "cache": false
    },
    "generate": {
      "outputs": [
        "src/data/**"
      ],
      "cache": false
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": ["coverage/**"]
    },
    "test:coverage": {
      "dependsOn": ["build"],
      "inputs": [
        "**/*.test.ts",
        "**/*.test.tsx"
      ],
      "outputs": [
        "coverage/**"
      ]
    },
    "lint": {}
  }
}
